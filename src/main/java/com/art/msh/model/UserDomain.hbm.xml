<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.art.msh.model">
    <class name="UserDomain" table="user">
        <id name="id" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property name="account" length="15"/>
        <property name="password" length="15"/>
        <property name="idCard" length="15"/>
        <property name="userIcon" length="15"/>
        <property name="userName" length="25"/>
        <property name="userSex" length="25"/>
        <property name="userNotes" length="25"/>
    </class>
</hibernate-mapping>